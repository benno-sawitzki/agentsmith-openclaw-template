{
  "gateway": {
    "port": "${OPENCLAW_PORT:-4000}",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "workspace": "./workspace",
  "channels": {
    "telegram": {
      "enabled": "${TELEGRAM_ENABLED:-false}",
      "token": "${TELEGRAM_BOT_TOKEN}"
    },
    "whatsapp": {
      "enabled": "${WHATSAPP_ENABLED:-false}"
    }
  }
}
